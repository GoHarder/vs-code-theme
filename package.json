{
  "name": "goharder-vscode-theme",
  "version": "0.0.4",
  "description": "GoHarder theme for visual studio code.",
  "scripts": {
    "dev-dark": "cd src/dark-default; nodemon index",
    "dev-dark-plus": "cd src/dark-plus; nodemon index",
    "package": "npx vsce package",
    "publish": "npx vsce publish",
    "login": "npx vsce login GoHarder",
    "ts-watch": "tsc -w -p src"
  },
  "author": "Gregory Harder",
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@material/material-color-utilities": "^0.2.7",
    "@types/inquirer": "^9.0.7",
    "chroma-js": "^2.4.2",
    "commander": "^11.1.0",
    "inquirer": "^9.2.14",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GoHarder/vs-code-theme"
  },
  "publisher": "GoHarder",
  "displayName": "GoHarder Theme",
  "icon": "icon.png",
  "categories": [
    "Themes"
  ],
  "keywords": [
    "theme",
    "dark",
    "GoHarder"
  ],
  "engines": {
    "vscode": "^1.85.0"
  },
  "contributes": {
    "themes": [
      {
        "label": "GoHarder Dark Default",
        "uiTheme": "vs-dark",
        "path": "./themes/dark-default.json"
      },
      {
        "label": "GoHarder Dark+",
        "uiTheme": "hc-black",
        "path": "./themes/dark-plus.json"
      }
    ]
  }
}
